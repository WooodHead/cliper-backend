    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "config": "^1.21.0",
    "http": "0.0.0",
    "jquery": "^3.1.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.3",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
